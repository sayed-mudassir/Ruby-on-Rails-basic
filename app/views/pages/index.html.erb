<h1>pages</h1>
<%= link_to 'new', new_page_path %>
<% @pages.each do |page| %>
	<%= page.title %>
	<%= page.body %>
	<%= page.page_num %>

	
	<%= link_to 'edit', edit_page_path(page) %>
	<%= link_to 'show',  page_path(page)  %>
	<%= link_to 'delete', page, method: :delete , data:{confirm: "are you sure"}  %>
<%end%>